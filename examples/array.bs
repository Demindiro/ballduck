fn main()
	env.print("Array construction:")
	var arr = [1, 2, 3]
	env.print("  ", arr)

	env.print("Setting array index:")
	arr[1] = "duck"
	env.print("  ", arr)

	env.print("Iterating arrays:")
	for e in arr
		env.print("  ", e)

	env.print("Using array elements in expressions:")
	arr = arr[0] + arr[2]
	env.print("  ", arr)
