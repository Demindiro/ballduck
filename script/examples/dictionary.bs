fn main()
	print("Dictionary construction:")
	var dict = {1: 2, 3: "dog", "cat": "chicken"}
	print("  ", dict)

	print("Setting dictionary index:")
	dict[1] = "duck"
	print("  ", dict)

	print("Iterating dictionaries:")
	for k in dict
		print("  ", k, ": ", dict[k])

	print("Using dictionary elements in expressions:")
	var sum = dict[3] + " is not a " + dict["cat"]
	print("  ", sum)
